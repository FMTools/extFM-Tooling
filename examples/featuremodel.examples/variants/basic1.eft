featuremodel "basic"

domain <d1> [a1=0, a2= 1, a3= 2]
domain <d2> [0..3]

selected feature "root" <root>
	group <g1> (0..2) {
		selected feature "f1" <f1>
			attr1 [d1] := "a3"
			group <g2> (0..1) {
				deselected feature "f3" <f3>
				deselected feature "f4" <f4>
				}
		selected feature "f2" <f2>
			attr2 [d2] := "0"
		}
constraint <c1> f1.attr1 > f2.attr2
constraint <c3> f1.attr1 == "a3"
