featuremodel "basic"

domain <d1> [a1=0,a2=1,a3=2]
domain <d2> [0..3]

feature "someName" <someId>
group <g1> (0..2) {
	feature "s1" <s1Id>
        group <g2> (0..1) {
			feature "s3" <s3Id>
        }
		attr1 [d1]
		attr2 [d2]
	feature "s2" <s2Id>
		attr1 [d1]
		//attr2 [d2]
	//group <g2> (0..1) {
	//	feature "s2" <s2Id>
	//}
}

constraint <c1> s1Id.attr1 > s1Id.attr2
constraint <c3> s1Id.attr1 == "a3"
constraint <c2> s1Id -> s3Id
