featuremodel "basic"

domain <d1> [a1=0,a2=1,a3=2]
domain <d2> [0..3]

selected feature "root" <root>
group <g1> (0..2) {
	feature "f1" <f1>
        group <g2> (0..1) {
			feature "f3" <f3>
        }
		attr1 [d1]
	feature "f2" <f2>
		attr1 [d2]
		//attr2 [d2]
	//group <g2> (0..1) {
	//	feature "s2" <s2Id>
	//}
}

constraint <c1> f1.attr1 > f2.attr1
constraint <c3> f1.attr1 == "a3"
