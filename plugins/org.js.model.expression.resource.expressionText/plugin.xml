<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.js.model.expression.resource.expressionText.mopp.ExpressionTextMetaInformation" id="org.js.model.expression.resource.expressionText.mopp.ExpressionTextMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="org.js.model.expression.resource.expressionText.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="org.js.model.expression.resource.expressionText.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.js.model.expression.resource.expressionText.problem">
		</super>
	</extension>
	
	<extension id="org.js.model.expression.resource.expressionText.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.js.model.expression.resource.expressionText.problem">
		</super>
	</extension>
	
	<extension id="org.js.model.expression.resource.expressionText.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.js.model.expression.resource.expressionText.problem">
		</super>
	</extension>
	
	<extension id="org.js.model.expression.resource.expressionText.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.js.model.expression.resource.expressionText.problem">
		</super>
	</extension>
	
	<extension id="org.js.model.expression.resource.expressionText.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.js.model.expression.resource.expressionText.problem">
		</super>
	</extension>
	
	<extension id="org.js.model.expression.resource.expressionText.nature" name="expressionText nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.js.model.expression.resource.expressionText.mopp.ExpressionTextNature">
			</run>
		</runtime>
		<builder id="org.js.model.expression.resource.expressionText.builder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.js.model.expression.resource.expressionText.builder" name="expressionText Builder">
		<builder hasNature="true">
			<run class="org.js.model.expression.resource.expressionText.mopp.ExpressionTextBuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension-point id="org.js.model.expression.resource.expressionText.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.js.model.expression.resource.expressionText.mopp.ExpressionTextResourceFactoryDelegator" type="expressionText">
		</parser>
	</extension>
	
	<extension-point id="org.js.model.expression.resource.expressionText.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd">
	</extension-point>
	
	<extension point="org.eclipse.debug.core.breakpoints">
		<breakpoint class="org.js.model.expression.resource.expressionText.debug.ExpressionTextLineBreakpoint" id="org.js.model.expression.resource.expressionText.debug.breakpoint" markerType="org.js.model.expression.resource.expressionText.debug.lineBreakpoint.marker" name="expressionText Breakpoint">
		</breakpoint>
	</extension>
	
	<extension id="org.js.model.expression.resource.expressionText.debug.lineBreakpoint.marker" point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.debug.core.lineBreakpointMarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<persistent value="true">
		</persistent>
	</extension>
	
</plugin>
