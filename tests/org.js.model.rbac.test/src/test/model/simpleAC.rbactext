access control on <simpleFM.eft> 
references <ac.rbactext>

role <r1> {
	"select f2",
	task "t1" <t1>,
	"select f3"
}

role <r2> extends r1 {
	"deselect f2"
}

role <r3> {
	"select f2",
	"deselect f2"
}

role <r4> extends r2 

role <r5> {
	"select f9",
	"deselect f9",
	"select f4",
	"deselect f4"
}

role <r6> extends r5, r4, r3 {
	"select f5",
	"deselect f5"
}

role <r7> {
	"set f5.a1",
	"set f6.a1"
}

group <r7> r7 {r6, r4}
group <r8> r6 {r3, r2}


subject "s1" <s1> {r1}

subject "s2" <s2> {r2}

subject "s3" <s3> {r2, r1}

subject "s4" <s4> {r2, r1} 
 	
subject "s5" <s5> {r4}