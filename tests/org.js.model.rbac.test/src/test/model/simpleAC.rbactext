access control on <simpleFM.eft>

role <r1> {
	f2 : select
}

role <r2> extends r1 {
	f2 : deselect
}

role <r3> {
	f2 : select deselect
}

role <r4> extends r2 {
}

role <r5> extends r3 {
	f9 : select deselect
	f4 : select deselect
}

role <r6> extends r4, r5 {
	f5 : select deselect
}

subject "s1" <s1> {
 	r1
}

subject "s2" <s2> {
 	r2
}

subject "s3" <s3> {
 	r2 r1
}

subject "s4" <s4> {
 	r1 r2
}

subject "s5" <s5> {
 	r4
}